@{
    Layout = "";
}
@model Beeant.Cloud.Crm.Mobile.Models.Department.DepartmentListModel
@if (Model != null && Model.Departments != null)
{
    foreach (var es in Model.Departments)
    {
        <div class="element" Sequence="@es.Sequence" DataId="@es.Id">
            <div class="up"></div>
            <div class="name"> @es.Name</div>
            <div class="down" ></div>
            <input class="remove" type="button" style="display: none;" value="删除" />
        </div>
    }
}