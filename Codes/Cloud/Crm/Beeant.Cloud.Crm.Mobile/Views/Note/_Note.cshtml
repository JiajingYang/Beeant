@{
    Layout = "";
}
@model Beeant.Cloud.Crm.Mobile.Models.Note.NoteListModel
@if (Model != null && Model.Notes != null)
{
    foreach (var note in Model.Notes)
    {
        <div class="element" DataId="@note.Id">

            <div class="name"> @note.InsertTime.ToString("yyyy-MM-dd")@note.Name:@note.Content</div>

            <input class="remove" type="button" style="display: none;" value="删除" />
        </div>
    }
}