@{
    Layout = "";
}
@using Beeant.Basic.Services.Mvc.Extension
@using Beeant.Basic.Services.Mvc.Extension.Mobile
@using Beeant.Basic.Services.Mvc.Script
@using Beeant.Domain.Entities
@using Beeant.Domain.Entities.Site
@using Winner.Persistence
@model Beeant.Cloud.Site.Mobile.Models.Qrcode.QrcodeListModel
@if (Model != null && Model.Commodities != null)
{
    foreach (var commodity in Model.Commodities)
    {
        <div class="element">
            <div class="name">@commodity.Name</div>
            <img src="@Html.GetPreRenderPicture()" data-original="@(string.IsNullOrEmpty(commodity.FileName) ? Html.GetNoPicture() : commodity.FullFileName)" class="img" alt="" />
            <img src="@Html.GetPreRenderPicture()" data-original="/Qrcode/CommodityQrCode/@ViewBag.SiteId?id=@commodity.Id" class="img" alt="" />

        </div>
    }
}