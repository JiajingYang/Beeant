<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <Dislan>
    <XmlLanguage>
      <!--////////////////////////////枚举////////////////////////////////////////-->
    
      <!--星期-->
      <Language Name="System.DayOfWeek">
        <Message Name="Monday" Value="星期1"></Message>
        <Message Name="Tuesday" Value="星期2"></Message>
        <Message Name="Wednesday" Value="星期3"></Message>
        <Message Name="Thursday" Value="星期4"></Message>
        <Message Name="Friday" Value="星期5"></Message>
        <Message Name="Saturday" Value="星期6"></Message>
        <Message Name="Sunday" Value="星期7"></Message>
      </Language>
      <!--来源类型-->
      <Language Name="Beeant.Domain.Entities.ChannelType">
        <Message Name="Admin" Value="后台"></Message>
        <Message Name="Website" Value="网站"></Message>
        <Message Name="Mobile" Value="微网站"></Message>
        <Message Name="Api" Value="接口"></Message>
        <Message Name="Ios" Value="Ios"></Message>
        <Message Name="Android" Value="安卓"></Message>
      </Language>
      <Language Name="Beeant.Domain.Entities.BaseEntity.StatusName">
        <Message Name="True" Value="是"></Message>
        <Message Name="False" Value="否"></Message>
      </Language>
		<Language Name="Beeant.Domain.Entities.BaseEntity.ReplayName">
			<Message Name="True" Value="已处理"></Message>
			<Message Name="False" Value="未处理"></Message>
		</Language>
      <Language Name="Beeant.Domain.Entities.BaseEntity.VerifyName">
        <Message Name="True" Value="启用"></Message>
        <Message Name="False" Value="禁止"></Message>
      </Language>
      <Language Name="Beeant.Domain.Entities.BaseEntity.ShowName">
        <Message Name="True" Value="展示"></Message>
        <Message Name="False" Value="隐藏"></Message>
      </Language>
      <Language Name="Beeant.Domain.Entities.BaseEntity.SalesName">
        <Message Name="True" Value="销售"></Message>
        <Message Name="False" Value="下架"></Message>
      </Language>
      <Language Name="Beeant.Domain.Entities.BaseEntity.ServiceName">
        <Message Name="True" Value="开启"></Message>
        <Message Name="False" Value="停止"></Message>
      </Language>
    </XmlLanguage>
  </Dislan>
</configuration>                                                                                                                                           