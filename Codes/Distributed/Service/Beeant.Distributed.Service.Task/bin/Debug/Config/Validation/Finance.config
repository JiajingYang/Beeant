          <Validation RuleName="PrimaryKey"  ValidationType="Add|Modify"></Validation>
          <Validation RuleName="Requiry"  ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="ChannelTypeName" Message="来源类型必选">
          <Validation RuleName="Requiry" ValidationType="Add|Modify"></Validation>
