<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <Filter>
    <XmlValidation>
      <!--登入日志-->
      <Model Name="Beeant.Domain.Entities.Log.LoginEntity">
        <Property PropertyName="Type" Message="类型长度不能超过50">
          <Validation RuleName="LengthRange" P0="0" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Address" Message="地址长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Device" Message="设备号长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="City" Message="城市长度不能超过50">
          <Validation RuleName="LengthRange" P0="0" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Ip" Message="Ip长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Message" Message="消息长度不能超过200">
          <Validation RuleName="LengthRange" P0="0" P1="200" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="AccountId" Message="用户编号不正确">
          <Validation RuleName="PrimaryKey" ValidationType="Add|Modify"></Validation>
        </Property>
      </Model>
      <!--错误日志-->
      <Model Name="Beeant.Domain.Entities.Log.ErrorEntity">
        <Property PropertyName="Message" Message="错误信息长度不能超过4000">
          <Validation RuleName="LengthRange" P0="0" P1="4000" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Detail" Message="错误描述长度不能超过1000000">
          <Validation RuleName="LengthRange" P0="0" P1="1000000" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Address" Message="地址长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Ip" Message="Ip长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Device" Message="设备号长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="AccountId" Message="用户编号不正确">
          <Validation RuleName="PrimaryKey" ValidationType="Add|Modify"></Validation>
        </Property>
      </Model>
      <!--操作日志-->
      <Model Name="Beeant.Domain.Entities.Log.OperationEntity">
        <Property PropertyName="Type" Message="类型长度不能超过50">
          <Validation RuleName="LengthRange" P0="0" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Name" Message="名称长度不能超过50">
          <Validation RuleName="LengthRange" P0="0" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Control" Message="控件名称长度不能超过50">
          <Validation RuleName="LengthRange" P0="0" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Detail" Message="错误描述长度不能超过1000000">
          <Validation RuleName="LengthRange" P0="0" P1="1000000" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Address" Message="地址长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Ip" Message="Ip长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="AccountId" Message="用户编号不正确">
          <Validation RuleName="PrimaryKey" ValidationType="Add|Modify"></Validation>
        </Property>
      </Model>
      <!--消息日志-->
      <Model Name="Beeant.Domain.Entities.Log.MessageEntity">
        <Property PropertyName="Name" Message="错误名称长度不能超过50">
          <Validation RuleName="LengthRange" P0="0" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Content" Message="错误描述长度不能超过4000">
          <Validation RuleName="LengthRange" P0="0" P1="4000" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="ToAddress" Message="地址长度不能超过20000">
          <Validation RuleName="LengthRange" P0="0" P1="20000" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="FromAddress" Message="地址长度不能超过1000">
          <Validation RuleName="LengthRange" P0="0" P1="1000" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Status" Message="状态长度不能超过500">
          <Validation RuleName="LengthRange" P0="0" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
      </Model>
    </XmlValidation>
  </Filter>
</configuration>