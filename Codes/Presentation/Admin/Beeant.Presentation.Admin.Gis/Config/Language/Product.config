<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <Dislan>
    <XmlLanguage>
      <!--产品属性类型-->
      <Language Name="Beeant.Domain.Entities.Product.PropertyType">
        <Message Name="None" Value="无"></Message>
        <Message Name="Input" Value="输入框"></Message>
        <Message Name="Select" Value="单选框"></Message>
        <Message Name="Check" Value="多选框"></Message>
        <Message Name="Combo" Value="组合框"></Message>
      </Language>
      <!--产品属性搜索类型-->
      <Language Name="Beeant.Domain.Entities.Product.PropertySearchType">
        <Message Name="None" Value="无"></Message>
        <Message Name="Select" Value="单项搜索"></Message>
        <Message Name="Check" Value="多选搜索"></Message>
      </Language>
      <!--属性规则类型-->
      <Language Name="Beeant.Domain.Entities.Product.PropertyRuleType">
        <Message Name="Add" Value="添加"></Message>
        <Message Name="Modify" Value="修改"></Message>
        <Message Name="Remove" Value="删除"></Message>
      </Language>
      <!--产品平台同步类型-->
      <Language Name="Beeant.Domain.Entities.Product.PlatformType">
        <Message Name="Taobao" Value="淘宝"></Message>
      </Language>
      <!--点评类型-->
      <Language Name="Beeant.Domain.Entities.Product.CommentType">
        <Message Name="Good" Value="好评"></Message>
        <Message Name="General" Value="中评"></Message>
        <Message Name="Bad" Value="差评"></Message>
      </Language>
      <!--供应链产品状态-->
      <Language Name="Beeant.Domain.Entities.Product.ProductLogStatusType">
        <Message Name="UnSubmit" Value="未报送"></Message>
        <Message Name="GoodsExamine" Value="产品审核"></Message>
        <Message Name="OperateExamine" Value="运营审核"></Message>
        <Message Name="Normal" Value="正常"></Message>
      </Language>
      <!--供应链销售状态-->
      <Language Name="Beeant.Domain.Entities.Product.ProductLogSalesStatusType">
        <Message Name="UnSales" Value="下架"></Message>
        <Message Name="SalesExamine" Value="上架申请"></Message>
        <Message Name="ReturnSales" Value="上架退回"></Message>
        <Message Name="Normal" Value="正常销售"></Message>
      </Language>
      <!--供应链审核类型-->
      <Language Name="Beeant.Domain.Entities.Product.ProductLogType">
        <Message Name="Price" Value="价格审核"></Message>
        <Message Name="Sales" Value="上架审核"></Message>
      </Language>
    </XmlLanguage>
  </Dislan>
