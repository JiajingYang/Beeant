<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <Filter>
    <XmlValidation>
      <!--配送站信息-->
      <Model Name="Beeant.Domain.Entities.Gis.AreaEntity">
        <Property PropertyName="Name" Message="名字必填而且长度不能超过50">
          <Validation RuleName="LengthRange" P0="1" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Name" Message="名字必填而且长度不能超过50">
          <Validation RuleName="LengthRange" P0="1" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="City" Message="城市必填而且长度不能超过50">
          <Validation RuleName="LengthRange" P0="1" P1="50" ValidationType="Add"></Validation>
        </Property>
        <Property PropertyName="Type" Message="地图类型必须">
          <Validation RuleName="LengthRange" P0="1" P1="50" ValidationType="Add"></Validation>
        </Property>
        <Property PropertyName="Color" Message="颜色必须">
          <Validation RuleName="LengthRange" P0="1" P1="50" ValidationType="Add"></Validation>
        </Property>
        <Property PropertyName="Path" Message="边界范围长度不能超过8000">
          <Validation RuleName="LengthRange" P0="0" P1="8000" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Value" Message="值不能超过50个字符">
          <Validation RuleName="LengthRange" P0="0" P1="50" ValidationType="Add"></Validation>
        </Property>
        <Property PropertyName="Remark" Message="备注长度不能超过100">
          <Validation RuleName="LengthRange" P0="0" P1="100" ValidationType="Add|Modify"></Validation>
        </Property>
      </Model>
      <!--地址信息-->
      <Model Name="Beeant.Domain.Entities.Gis.AddressEntity">
        <Property PropertyName="Name" Message="名字必填而且长度不能超过20">
          <Validation RuleName="LengthRange" P0="1" P1="500" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="Point" Message="地址经纬度长度不能超过1000">
          <Validation RuleName="LengthRange" P0="0" P1="1000" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="City" Message="城市长度不能超过50">
          <Validation RuleName="LengthRange" P0="1" P1="50" ValidationType="Add|Modify"></Validation>
        </Property>
        <Property PropertyName="NameExist" Message="地址已经存在"></Property>
      </Model>
    </XmlValidation>
  </Filter>
</configuration>