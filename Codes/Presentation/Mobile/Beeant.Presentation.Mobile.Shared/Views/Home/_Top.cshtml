@using Beeant.Basic.Services.Mvc.Extension
@using Beeant.Basic.Services.Mvc.Script
function InitTop(){
document.write('<meta charset="utf-8"/>');
document.write('<meta name="format-detection" content="telephone=no">');
document.write('<meta name ="viewport" content="width=device-width,initial-scale=1.0,maximum-scale=1.0,user-scalable=0" />');
document.write('<link rel="shortcut icon" href="@Html.GetUrl("PresentationMobileSharedUrl")/images/favicon.ico" type="image/x-icon"/>');
document.write('<link href="@Html.GetUrl("PresentationMobileSharedUrl")/Content/site.css" rel="stylesheet" />');
document.write('<script type="text/javascript" src="@Html.GetUrl("PresentationMobileSharedUrl")/Scripts/Plug/jquery-1.7.1.min.js"></script>');
document.write('<script type="text/javascript" src="@Html.GetUrl("PresentationMobileSharedUrl")/Scripts/Winner/Winner.ClassBase.js"></script>');
document.write('<script type="text/javascript" src="@Html.GetUrl("PresentationMobileSharedUrl")/Scripts/Winner/DataLoader/Winner.DataLoader.js"></script>');
document.write('<script type="text/javascript" src="@Html.GetUrl("PresentationMobileSharedUrl")/Scripts/Plug/jquery.lazyload.min.js"></script>');
document.write('<script type="text/javascript"> window.lazyloadImage = function (container) { var imgs =(container== null ? $("img") : $(container).find("img")); imgs.lazyload({ placeholder: "@Html.GetUrl("PresentationMobileSharedUrl")/images/logo.png", effect: "fadeIn", failurelimit: 10, threshold: 200 }); };</script>');
}
