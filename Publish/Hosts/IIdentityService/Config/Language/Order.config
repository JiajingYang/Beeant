<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <Dislan>
    <XmlLanguage>
      <!--订单流程-->
      <Language Name="Beeant.Domain.Entities.Order.OrderStatusType">
        <Message Name="Cancel" Value="取消"></Message>
        <Message Name="Finish" Value="完成"></Message>
        <Message Name="Submit" Value="已经提交"></Message>
        <Message Name="WaitPay" Value="等待支付"></Message>
        <Message Name="WaitPay" Value="等待支付"></Message>
        <Message Name="WaitDelivery" Value="等待发货"></Message>
        <Message Name="WaitSign" Value="等待确认"></Message>
        <Message Name="WaitSettle" Value="等待结算"></Message>
      </Language>
      <!--订单来源类型-->
      <Language Name="Beeant.Domain.Entities.Order.OrderType">
        <Message Name="Standard" Value="标准"></Message>
        <Message Name="Custom" Value="定制"></Message>
        <Message Name="Integral" Value="积分兑换"></Message>
        <Message Name="Return" Value="退换货"></Message>
      </Language>
      <!--订单明细类型-->
      <Language Name="Beeant.Domain.Entities.Order.OrderItemType">
        <Message Name="Product" Value="产品"></Message>
        <Message Name="Express" Value="运费"></Message>
        <Message Name="Deduction" Value="抵扣"></Message>
        <Message Name="Else" Value="其它"></Message>
      </Language>
      <!--订单投诉评价类型-->
      <Language Name="Beeant.Domain.Entities.Order.ComplaintType">
        <Message Name="None" Value="没有评价"></Message>
        <Message Name="Good" Value="好评"></Message>
        <Message Name="General" Value="中评"></Message>
        <Message Name="Bad" Value="差评"></Message>
      </Language>
      <!--服务工单服务类型-->
      <Language Name="Beeant.Domain.Entities.Order.ServiceType">
        <Message Name="Repair" Value="返修"></Message>
        <Message Name="Return" Value="退货"></Message>
        <Message Name="Barter" Value="换货"></Message>
      </Language>
      <!--服务工单最终返回方式-->
      <Language Name="Beeant.Domain.Entities.Order.ServiceReturnType">
        <Message Name="Express" Value="快递送回"></Message>
        <Message Name="DoorSelf" Value="上门自取"></Message>
      </Language>
      <!--服务工单状态-->
      <Language Name="Beeant.Domain.Entities.Order.ServiceStatusType">
        <Message Name="Submit" Value="提交申请"></Message>
        <Message Name="Auditing" Value="平台审核"></Message>
        <Message Name="Delivery" Value="平台收货"></Message>
        <Message Name="Handle" Value="平台处理"></Message>
        <Message Name="Accomplish" Value="完成"></Message>
      </Language>
      <!--订单通知-->
      <Language Name="Beeant.Domain.Entities.Order.Message">
        <Message Name="CancelTitle" Value="订单取消通知"></Message>
        <Message Name="CancelTitle" Value="订单取消通知"></Message>
        <Message Name="HandleTitle" Value="订单下达成功通知"></Message>
        <Message Name="DeliveryTitle" Value="订单付款通知"></Message>
        <Message Name="CancelDetail" Value="您提交的订单，订单号：{0}订单已取消，感谢您对我们平台的支持，谢谢。"></Message>
        <Message Name="WaitPaymentDetail" Value="您提交的订单，订单号：{0}已由仓库发货，请耐心等待，并保持手机通畅，感谢您对我们平台的支持，谢谢。"></Message>
        <Message Name="DeliveryDetail" Value="您提交的订单，订单号：{0}需要您付款, 付款金额为{1}，请在十个工作日内付款，逾期订单将取消，感谢您对我们平台的支持，谢谢。"></Message>
        <Message Name="HandleDetail" Value="您提交的订单，订单号：{0}已下达成功, 客服人员将会尽快与您确认订单金额，并保持手机通畅，感谢您对我们平台的支持，谢谢。"></Message>
      </Language>
      <Language Name="Beeant.Domain.Entities.Complaint.Message">
        <Message Name="Title" Value="客服投诉回复通知"></Message>
        <Message Name="Detail" Value="您提交的投诉，订单号：{0}已回复, 感谢您对我们平台的支持，谢谢。"></Message>
      </Language>
      <Language Name="Beeant.Domain.Entities.Service.Message">
        <Message Name="Title" Value="服务工单受理通知"></Message>
        <Message Name="Detail" Value="订单号：{0}对应工单状态已变更, 感谢您对我们平台的支持，谢谢。"></Message>
      </Language>
    </XmlLanguage>
  </Dislan>
</configuration>                                                                                                                                           